{
  "expo": {
    "name": "CAL-Assessment",
    "slug": "calassessment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/assessment-logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.cal2009.calassessment",
      "supportsTablet": true
    },
    "android": {
      "package": "com.cal2009.calassessment",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
